<?xml version="1.0" encoding="UTF-8"?>
<!-- web.xml -->
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>LearnHub</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>StudentProfile</display-name>
    <servlet-name>StudentProfile</servlet-name>
    <servlet-class>StudentProfile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentProfile</servlet-name>
    <url-pattern>/StudentProfile</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>StudentMainPage</display-name>
    <servlet-name>StudentMainPage</servlet-name>
    <servlet-class>StudentMainPage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentMainPage</servlet-name>
    <url-pattern>/StudentMainPage</url-pattern>
  </servlet-mapping>
</web-app>